import java.io.IOException;
import java.util.Scanner;

public class Main {
 
    public static void main(String[] args) throws IOException {
 
    		Scanner scan = new Scanner(System.in);
		
		int x,y;
				
		do 
		{
			x=scan.nextInt();
			y=scan.nextInt();
			
			if(x>y)
			{
				System.out.println("Decrescente");
			}
			else if(x<y)
			{
				System.out.println("Crescente");
			}
		}
		while(x!=y);
 
    }
 
}
