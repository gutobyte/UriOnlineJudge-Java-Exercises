import java.io.IOException;
import java.util.Scanner;
 

public class Main {
 
    public static void main(String[] args) throws IOException {
 
       Scanner scan = new Scanner(System.in);
       Scanner scan = new Scanner(System.in);
		
		int numero;
		
		numero=scan.nextInt();
		
		int result=2*numero;
		
		System.out.println(result + " minutos");
			

	}

}
