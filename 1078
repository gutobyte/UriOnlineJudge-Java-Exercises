import java.io.IOException;
import java.util.Scanner;

public class Main {
 
    public static void main(String[] args) throws IOException {
 
       Scanner scan = new Scanner(System.in);
		
		int valor=0, soma=0;
		
		valor=scan.nextInt();
		
		for(int i=1;i<=10;i++)
		{
			
			System.out.println(i + " x " + valor + " = " + (i*valor));
			
			
		}
	
 
    }
 
}
