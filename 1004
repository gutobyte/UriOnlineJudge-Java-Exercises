import java.io.IOException;
import java.util.Scanner;

public class Main {
 
    public static void main(String[] args) throws IOException {
 
        int A, B, PROD;
        
        Scanner scan = new Scanner(System.in);
       
       A = scan.nextInt();
       B = scan.nextInt();
       
       PROD = A * B;
       
       System.out.println("PROD = " + PROD);
 
    }
 
}
